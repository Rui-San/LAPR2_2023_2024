SS@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false
'skinparam linetype ortho
'left to right direction
skinparam classAttributeIconSize 0
hide circle
hide methods

title Domain Model

class Organization {
    name
    address
    nif
    website
    phoneNumber
    email
}

class Address{
    street
    streetNumber
    postalCode
    city
    district
}

class Collaborator{
    name
    admissionDate
    birthDate
    address
    phoneNumber
    email
    idDocType
    idNumber
    skills
    job
}

class HRM{

}

class FM{

}

class Skill{
    skillName
    description
}

class Job{
    jobName
}

class Vehicle{
    plateId
    brand
    model
    type
    tare
    grossWeight
    currentKm
    registerDate
    acquisitionDate
    checkupFrequencyKms
}

class Checkup{
    plateId
    date
    description
}

class Team_Proposal{
    minTeamSize
    maxTeamSize
    setOfSkills
}

class Team{
    name
    description
    members
}

class Agenda{
    date
    tasks
}

class Task{
    title
    duration
    status
    priority
}

class Portal_User{
    username
    password
    email
}

class GSU{

}

class Portal{
    name
    description
    url
}

class Comment{
    date
    text
    author
}

class Green_Space{
    name
    address
    type
    description
}

class SetOfSkills{
    quantity
    skillName
}

class System_Admin{

}

class System_User{
    username
    password
    email
}

class GSM{

}

Collaborator --up|> System_User
System_Admin "1" -- "*" System_User : "manages >"
System_User "1" -- "*" Role : "has >"
Green_Space "1" -- "1" Address : "has >"
GSU --|> Portal_User
GSM -|> Role
Portal_User "1" --right "*" Comment : "registers >"
Portal "1" --down "*" Comment : "has >"
Portal "1" -- "*" Portal_User : "has >"
Organization "1" ---down "*" Collaborator : "has >"
Organization "1" --left "*" Green_Space : "manages >"
Organization "1" -- "1" Address : "has >"
Collaborator "1" --left "1" Address : "has >"
Collaborator "1" -- "*" Skill : "has >"
Collaborator "1" -- "1..*" Job : "has >"
HRM "1" -- "*" Collaborator : "registers >"
HRM "1" --left "*" Job : "registers >"
HRM "1" -- "*" Skill : "registers >"
HRM --up|> Role
HRM "1" -- "*" Team_Proposal : "registers >"
Team_Proposal "1" --right "0..1" Team : "generates >"
Team_Proposal "1" --down "1..*" SetOfSkills : "has >"
Team "1" --down "*" Task : "has >"
SetOfSkills "1" --- "1..*" Skill : "has >"
Agenda "1" -- "*" Task : "contain >"
FM --left|> Role
FM "1" -- "*" Vehicle : "registers >"
FM "1" -- "*" Checkup : "registers >"
Vehicle "1" --right "*" Checkup : "has >"
Team "1" ---up "*" Collaborator : "has >"
Organization "1" --right "*" Portal : "owns >"

@enduml